#!/usr/bin/expect

# Runs a program from the RTI TUI

spawn linuxbbx
expect "License Agreement" { send <F8> }
expect "Employee Number" { send "9999\r" }
expect "N/Y: " { send "\r" }
expect "Teleflora" { send <F11> }
expect "Enter Program:" { send "/tmp/EM_PW\r" }
expect "Teleflora" { send <F10> }
expect "Y/N: " { send "Y\r" }
interact
